              <NavLink\n                            to=\"/\" \n                            exact\n                            activeStyle={{\n                            textDecoration: \"underline\",\n                            }}> \n                        <img id=\"header-logo\" src={logo} alt=\"logo\" title=\"Zest\" />\n                        </NavLink>\n                </div>\n                <ul id=\"nav-right\">\n                    <li><NavLink\n                            to=\"/recipes\" \n                            exact\n                            activeStyle={{\n                            textDecoration: \"underline\",\n                            }}> \n                        Recipes\n                        </NavLink></li>\n                    <li><a href=\"https://www.kroger.com/food-tips/cooking-skills/101-simple-cooking-tips\" target=\"_blank\">Tips</a></li>\n                </ul>\n            </nav>\n        </div>\n    )\n}\n\nexport default Header",["44","45"],"/Users/marcivallario/Development/code/Mod3/project/zest/src/components/Banner.js",["46"],"import About from \"./About\"\n\nfunction Banner({ featuredRecipe }) {\n    return (\n        <div id=\"home\">\n            <div id=\"banner-container\">\n                \n            </div>\n            <div id=\"rotd-container\">\n                <div id=\"rotd-content\">\n                    <div id=\"rotd-text\">\n                        <h2>Recipe of the Day: {featuredRecipe.name}</h2>\n                        <div id=\"featured-details\">\n                            <div id=\"featured-ingredients\">\n                                <h6>Ingredients</h6>\n                                <ul>\n                                    {featuredRecipe.ingredients.map(ingredient => <li key={ingredient.id}>{ingredient.name}</li>)}\n                                </ul>\n                            </div>\n                            <div id=\"featured-instructions\">\n                                <h6>Instructions</h6>\n                                <ol>\n                                    {featuredRecipe.directions.map(direction => {\n                                        return (<li key={direction.text}>{direction.text}</li>)\n                                    })}\n                                </ol>\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"banner-img-container\">\n                        <img src={featuredRecipe.picture_url} alt=\"Receipe of the Day\" title=\"Recipe of the Day\" />\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Banner","/Users/marcivallario/Development/code/Mod3/project/zest/src/components/SortSearchBar.js",[],["47","48"],"/Users/marcivallario/Development/code/Mod3/project/zest/src/components/CardContainer.js",[],"/Users/marcivallario/Development/code/Mod3/project/zest/src/components/Card.js",["49","50"],"import trashcan from '../assets/trashcan.png'\n\nfunction Card({ recipe, onDelete, updateRecipes }) {\n\n    function handleDelete() {\n        fetch(`http://localhost:9292/recipes/${recipe.id}`, {\n            method: 'DELETE'\n            })\n        .then(onDelete(recipe.id));\n    }\n\n    function handleChange(e) {\n        fetch(`http://localhost:9292/recipes/${recipe.id}`, {\n            method: \"PATCH\",\n            headers: {\n                \"Content-Type\": \"application/json\",\n            },\n            body: JSON.stringify({\n                rating: e.target.value\n            }),\n            })\n        .then((r) => r.json())\n        .then(updateRecipes);\n    }\n\n    console.log('Card: ', recipe)\n\n    return (\n        <div className=\"card-container\">\n            <div className=\"card-img-container\">\n                <img src={recipe.picture_url}/>\n            </div>\n            <div className=\"card-details\">\n                <h2>{recipe.name}</h2>\n                <h4>{recipe.cuisine.name}</h4>\n                <h6>Ingredients</h6>\n                {recipe.ingredients.map(ingredient => {\n                    return (<li key={ingredient.id}>{recipe.ingredient_lists.find(list => list.ingredient_id === ingredient.id).quantity}{ingredient.name}</li>)\n                })}\n                <ol>\n                    {recipe.directions.map(direction => <li key={direction.id}>{direction.text}</li>)}\n                </ol>\n                <div>\n                    <label>Rating: </label>\n                    <select value={recipe.rating} name=\"rating\" onChange={handleChange}>\n                        <option value={1}>⭐</option>\n                        <option value={2}>⭐⭐</option>\n                        <option value={3}>⭐⭐⭐</option>\n                        <option value={4}>⭐⭐⭐⭐</option>\n                        <option value={5}>⭐⭐⭐⭐⭐</option>\n                    </select>\n                    </div>\n                <img className=\"trash\" src={trashcan} title=\"Delete recipe\" onClick={handleDelete}/>\n                \n            </div>\n        </div>\n    )\n}\n\nexport default Card","/Users/marcivallario/Development/code/Mod3/project/zest/src/components/About.js",[],{"ruleId":"51","severity":1,"message":"52","line":48,"column":25,"nodeType":"53","messageId":"54","endLine":48,"endColumn":123,"fix":"55"},{"ruleId":"56","replacedBy":"57"},{"ruleId":"58","replacedBy":"59"},{"ruleId":"60","severity":1,"message":"61","line":1,"column":8,"nodeType":"62","messageId":"63","endLine":1,"endColumn":13},{"ruleId":"56","replacedBy":"64"},{"ruleId":"58","replacedBy":"65"},{"ruleId":"66","severity":1,"message":"67","line":31,"column":17,"nodeType":"53","endLine":31,"endColumn":48},{"ruleId":"66","severity":1,"message":"67","line":53,"column":17,"nodeType":"53","endLine":53,"endColumn":101},"react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" (which implies rel=\"noopener\") is a sec